<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentStart">

    <fragment
        android:id="@+id/fragmentStart"
        android:name="com.ch.ni.an.invest.screens.FragmentListTitleAnime"
        android:label="FragmentStart" >
        <action
            android:id="@+id/action_fragmentStart_to_fragmentAnimeNameQuotes"
            app:destination="@id/fragmentAnimeNameQuotes"
            app:popUpTo="@id/fragmentStart"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_fragmentStart_to_fragmentMyQuotes"
            app:destination="@id/fragmentMyQuotes"
            app:popUpTo="@id/fragmentStart"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAnimeNameQuotes"
        android:name="com.ch.ni.an.invest.screens.FragmentQuotesByAnimeTitle"
        android:label="FragmentAnimeNameQuotes" >
        <action
            android:id="@+id/action_fragmentAnimeNameQuotes_to_fragmentStart"
            app:destination="@id/fragmentStart"
            app:popUpTo="@id/fragmentAnimeNameQuotes"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_fragmentAnimeNameQuotes_to_fragmentMyQuotes"
            app:destination="@id/fragmentMyQuotes"
            app:popUpTo="@id/fragmentAnimeNameQuotes"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAnimeStart"
        android:name="com.ch.ni.an.invest.screens.FragmentTenRandomQuotes"
        android:label="FragmentAnimeStart" >
        <action
            android:id="@+id/action_fragmentAnimeStart_to_fragmentAnimeNameQuotes"
            app:destination="@id/fragmentAnimeNameQuotes"
            app:popUpTo="@id/fragmentAnimeStart"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_fragmentAnimeStart_to_fragmentMyQuotes"
            app:destination="@id/fragmentMyQuotes"
            app:popUpTo="@id/fragmentStart"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/fragmentMyQuotes"
        android:name="com.ch.ni.an.invest.screens.FragmentFavouriteQuotes"
        android:label="FragmentMyQuotes" >
        <action
            android:id="@+id/action_fragmentMyQuotes_to_fragmentStart"
            app:destination="@id/fragmentStart"
            app:popUpTo="@id/fragmentMyQuotes"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>